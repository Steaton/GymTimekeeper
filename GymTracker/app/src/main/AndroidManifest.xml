<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.zeko.gymtimekeeper" >

    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:name="one.thing.well.gymtimekeeper.GymTimekeeperApplication">

        <activity android:name="one.thing.well.gymtimekeeper.display.MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <service
            android:name="one.thing.well.gymtimekeeper.location.GymTimekeeperLocationTrackingService"
            android:enabled="true"
            android:exported="false" >
        </service>
    </application>
</manifest>
